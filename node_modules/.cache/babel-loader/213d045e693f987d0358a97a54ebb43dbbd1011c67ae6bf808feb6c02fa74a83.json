ementById('text-data').value;\n            // 저장하고자하는 파일명\n            const filename = '텍스트파일명';\n            const element = document.createElement('a');\n            element.setAttribute('href', 'data:text/plain;charset=utf-8, ' + encodeURIComponent(text));\n            element.setAttribute('download', filename);\n            document.body.appendChild(element);\n            element.click();\n        },\n        */\n    },\n    created (){\n//        this.$store.commit('setCartList',null); // 장바구니 비우기\n//        this.$store.commit('setPay', null); // 결제 방법 비우기\n//        this.$store.commit('setUserVo', null); // 적립한 사람의 정보 지우기\n//        this.$store.commit('setSavePoint',null); // 적립된 포인트 정보 지우기\n        \n    }\n});\n</script>\n<style>\n</style>"],"mappings":";OAYoDA,UAA2C;OAC7CC,UAA2C;OAC5CC,UAA2C;;EAbvFC,KAAK,EAAC;AAAM;;EACRA,KAAK,EAAC;AAAmB;;EAErBA,KAAK,EAAC;AAAS;gCAChBC,mBAAA,CACM;EADDD,KAAK,EAAC;AAAU;gCAGrBC,mBAAA,CACM;EADDD,KAAK,EAAC;AAAW;;EAGjBA,KAAK,EAAC;AAAQ;gCACgBC,mBAAA,CAAkF;EAA7EC,GAA2C,EAA3CL,UAA2C;EAAEG,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;gCAC3EF,mBAAA,CAAqF;EAAhFC,GAA2C,EAA3CJ,UAA2C;EAAKE,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;gCAC7EF,mBAAA,CAAoF;EAA/EC,GAA2C,EAA3CH,UAA2C;EAAIC,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;;;uBAdvHC,mBAAA,CAAAC,SAAA,SACAJ,mBAAA,CAmBM,OAnBNK,UAmBM,GAlBFL,mBAAA,CAgBM,OAhBNM,UAgBM,GAfFC,mBAAA,+BAAkC,EAClCP,mBAAA,CAYM,OAZNQ,UAYM,GAXFC,UACM,EAENC,UACM,EAENV,mBAAA,CAIM,OAJNW,UAIM,GAHFC,YAAA,CAA+HC,sBAAA;IAAlHC,EAAE,EAAC;EAAc;IAZ9CC,OAAA,EAAAC,QAAA,CAY+C,MAAkF,CAAlFC,UAAkF,C;IAZjIC,CAAA;MAagBN,YAAA,CAAgIC,sBAAA;IAAnHC,EAAE,EAAC;EAAY;IAb5CC,OAAA,EAAAC,QAAA,CAa6C,MAAqF,CAArFG,UAAqF,C;IAblID,CAAA;MAcgBN,YAAA,CAA8HC,sBAAA;IAAjHC,EAAE,EAAC;EAAW;IAd3CC,OAAA,EAAAC,QAAA,CAc4C,MAAoF,CAApFI,UAAoF,C;IAdhIF,CAAA;YAmBIX,mBAAA,uBAA0B,C,GAE9BA,mBAAA,WAAc,C","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}