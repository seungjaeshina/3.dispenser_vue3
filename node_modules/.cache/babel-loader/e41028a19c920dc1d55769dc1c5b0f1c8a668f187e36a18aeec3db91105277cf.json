e.error(\"실패 \", res);\n      });\n    },\n    goAutoSelect() {\n      if (this.autoselect == '0') {\n        this.boxNo = boxNo;\n        this.log3_data.postNo = '1111101';\n        this.log3_data.boxNo = boxNo;\n        this.log3_data.userId = 'deli001';\n        this.log3_data.menuName = '등기투입';\n        this.log3_data.actionData = '함자동선택';\n      } else {}\n      // 로그처리\n      axios.post(\"http://localhost:8910/admin/dispenserlog/create\", this.log1_data, {\n        header: {\n          \"Context-Type\": \"multipart/form-data\"\n        }\n      }).then(res => {\n        // 성공했을 경우\n        console.log(\"성공\", res);\n      }).catch(res => {\n        // 실패했을 경우\n        console.error(\"실패 \", res);\n      });\n      this.$router.push('/reginoscan');\n    },\n    goDone() {\n      // 로그처리\n      axios.post(\"http://localhost:8910/admin/dispenserlog/create\", this.log2_data, {\n        header: {\n          \"Context-Type\": \"multipart/form-data\"\n        }\n      }).then(res => {\n        // 성공했을 경우\n        console.log(\"성공\", res);\n      }).catch(res => {\n        // 실패했을 경우\n        console.error(\"실패 \", res);\n      });\n      this.$router.push('/');\n    },\n    modalClose() {\n      this.$refs.modal_selectbox.style.display = 'none';\n    },\n    // 등기투입\n    postRegiNo() {\n      axios.post(\"https://postnet.co.kr/savePutNmDistrRegiMail.jsp\", this.request_data, {\n        header: {\n          \"Context-Type\": \"multipart/form-data\"\n        }\n      }).then(res => {\n        // 성공했을 경우\n        console.log(\"성공\", res);\n        // statusCd : 등기투입결과 - 1 - S성공,F실패\n      }).catch(res => {\n        // 실패했을 경우\n        console.error(\"실패 \", res);\n      });\n    }\n  },\n  created() {},\n  mounted() {\n    // 등기함 상태값 가져오기\n    this.box_data.postNo = '1111101';\n    axios.get(\"http://localhost:8910/admin/dispenser/status\", this.box_data, {\n      header: {\n        \"Context-Type\": \"multipart/form-data\"\n      }\n    }).then(res => {\n      // 성공했을 경우\n      console.log(\"등기함상태\", JSON.stringify(res.data));\n      this.boxData = res.data.data.items;\n      console.log(\"등기함상태2\", this.boxData);\n      if (res.data.errno == -1) {\n        this.$router.push('/selectbox');\n      } else {\n        this.$router.push('/selectbox');\n      }\n    }).catch(res => {\n      // 실패했을 경우\n      console.log(\"등기함상태\", res.errmsg);\n      alert('관리자에게 문의 바랍니다.');\n    });\n  }\n};","map":{"version":3,"names":["axios","name","data","autoselect","boxNo","regiNo","veriNo","box_data","log1_data","postNo","menuName","actionData","log2_data","log3_data","boxData","components","methods","goBack","$router","go","goHome","push","goClick","alert","userId","post","header","then","res","console","log","catch","error","goAutoSelect","goDone","modalClose","$refs","modal_selectbox","style","display","postRegiNo","request_data","created","mounted","get","JSON","stringify","items","errno","errmsg"],"sources":["D:\\0610\\1020_KoreaPost\\2.Dispenser\\02.Source\\3.dispenser_vue3\\src\\views\\input\\SelectBox.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"wrap\">\n\n            <!-- ======== 카테고리 헤더 ======== -->\n            <div class=\"header\">\n                <div class=\"main_logo\" v-on:click=\"goHome\"></div>\n                <div class=\"header_title\">\n                    <div class=\"header_title_down\">보관함 선택</div>    \n                </div>\n                <div class=\"back\"  v-on:click=\"goBack\"></div>    \n            </div>\n\n            <!-- ======== 내용 ======== -->\n            <div class=\"selectbox_container\">\n                <div class=\"selectbox_area\">\n<!--\n                    <div v-for=\"item in boxData\" :key=\"item.boxNo\">\n                       <v-if=\"item.boxNo === '101'\">\n                            <div class=\"selectbox_101_full\" v-on:click=\"goClick('101')\">{{ item.boxNo }}</div>\n                       <v-else>\n                            <div class=\"selectbox_101_empty