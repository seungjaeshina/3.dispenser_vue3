\n            const filename = '텍스트파일명';\n            const element = document.createElement('a');\n            element.setAttribute('href', 'data:text/plain;charset=utf-8, ' + encodeURIComponent(text));\n            element.setAttribute('download', filename);\n            document.body.appendChild(element);\n            element.click();\n        },\n        */\n    },\n    created (){\n//        this.$store.commit('setCartList',null); // 장바구니 비우기\n//        this.$store.commit('setPay', null); // 결제 방법 비우기\n//        this.$store.commit('setUserVo', null); // 적립한 사람의 정보 지우기\n//        this.$store.commit('setSavePoint',null); // 적립된 포인트 정보 지우기\n        \n    }\n});\n</script>\n<style>\n</style>"],"mappings":";OAYsDA,UAAqC;OACvCC,UAAqC;OACrCC,UAAqC;;EAbpFC,KAAK,EAAC;AAAM;;EACRA,KAAK,EAAC;AAAmB;;EAErBA,KAAK,EAAC;AAAS;gCAChBC,mBAAA,CACM;EADDD,KAAK,EAAC;AAAS;gCAGpBC,mBAAA,CACM;EADDD,KAAK,EAAC;AAAU;;EAGhBA,KAAK,EAAC;AAAQ;gCACkBC,mBAAA,CAA4E;EAAvEC,GAAqC,EAArCL,UAAqC;EAAEG,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;gCACrEF,mBAAA,CAA+E;EAA1EC,GAAqC,EAArCJ,UAAqC;EAAKE,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;gCACtEF,mBAAA,CAA8E;EAAzEC,GAAqC,EAArCH,UAAqC;EAAIC,KAAK,EAAC,aAAa;EAACG,EAAE,EAAC;;;;uBAdpHC,mBAAA,CAAAC,SAAA,SACAJ,mBAAA,CAmBM,OAnBNK,UAmBM,GAlBFL,mBAAA,CAgBM,OAhBNM,UAgBM,GAfFC,mBAAA,+BAAkC,EAClCP,mBAAA,CAYM,OAZNQ,UAYM,GAXFC,UACM,EAENC,UACM,EAENV,mBAAA,CAIM,OAJNW,UAIM,GAHFC,YAAA,CAA2HC,sBAAA;IAA9GC,EAAE,EAAC;EAAgB;IAZhDC,OAAA,EAAAC,QAAA,CAYiD,MAA4E,CAA5EC,UAA4E,C;IAZ7HC,CAAA;MAagBN,YAAA,CAA4HC,sBAAA;IAA/GC,EAAE,EAAC;EAAc;IAb9CC,OAAA,EAAAC,QAAA,CAa+C,MAA+E,CAA/EG,UAA+E,C;IAb9HD,CAAA;MAcgBN,YAAA,CAA2HC,sBAAA;IAA9GC,EAAE,EAAC;EAAc;IAd9CC,OAAA,EAAAC,QAAA,CAc+C,MAA8E,CAA9EI,UAA8E,C;IAd7HF,CAAA;YAmBIX,mBAAA,uBAA0B,C,GAE9BA,mBAAA,WAAc,C","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}