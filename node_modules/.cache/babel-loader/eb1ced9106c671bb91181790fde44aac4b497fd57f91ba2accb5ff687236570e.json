\n        return {\n            log_data :{},\n        };\n    },\n    component() {\n    },\n    methods: {\n        goBack(){\n            this.$router.go(-1);\n        },\n        goHome(){\n            this.$router.push('/');\n        },\n        kbd_click(){\n            document.querySelector('.phonepad_info').style.display = 'block';\n            document.querySelector('.myitem').value = 'sms';\n            document.querySelector('.myinput').value = document.querySelector('#sms_receiptno').innerText;        \n            document.getElementById('myinput').focus();\n            document.getElementById('myinput').click();\n        },\n        goNum(num){\n            var result = document.querySelector('#sms_receiptno').innerText;\n            if (num=='11') {\n                if (result.length > 0) {\n                    result = result.slice(0, -1); \n                    document.querySelector('#sms_receiptno').innerText = result;\n                }       \n            } else {\n                    result = document.querySelector('#sms_receiptno').innerText;\n                    result = result + '' + num; \n                    document.querySelector('#sms_receiptno').innerText = result;\n            }    \n        },\n        goReceiptSign(){\n            var result = document.querySelector('#sms_receiptno').innerText;\n            this.log_data.postNo = '1111101';\n            this.log_data.boxNo = '';\n            this.log_data.userId = '';\n            this.log_data.menuName = '[메뉴]인증번호입력';\n            this.log_data.actionData = result;\n\n            // 로그처리\n            axios.post(\"http://115.91.83.140:8910/admin/dispenserlog/create\", this.log_data, {\n                header: {\n                    \"Context-Type\": \"multipart/form-data\",\n                },\n            })\n            .then((res) => { // 성공했을 경우\n                console.log(\"성공\", res);\n\n            })\n            .catch((res) => { // 실패했을 경우\n                console.error(\"실패 \", res);\n            });\n            this.$router.push('/receiptsign');\n        },\n        modalClose() {\n            this.$refs.modal2.style.display = 'none'; \n        }\n    },\n\n    created() {\n    },\n    mounted() {\n    }\n\n};\n</script>\n<style></style>"],"mappings":";AAqEA,OAAO,8BAA8B;AACrC,OAAOA,KAAI,MAAO,OAAO;AACzB;;AAEA,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAO,EAAG,CAAC;IACf,CAAC;EACL,CAAC;EACDC,SAASA,CAAA,EAAG,CACZ,CAAC;EACDC,OAAO,EAAE;IACLC,MAAMA,CAAA,EAAE;MACJ,IAAI,CAACC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC;IACvB,CAAC;IACDC,MAAMA,CAAA,EAAE;MACJ,IAAI,CAACF,OAAO,CAACG,IAAI,CAAC,GAAG,CAAC;IAC1B,CAAC;IACDC,SAASA,CAAA,EAAE;MACPC,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAO;MAChEH,QAAQ,CAACC,aAAa,CAAC,SAAS,CAAC,CAACG,KAAI,GAAI,KAAK;MAC/CJ,QAAQ,CAACC,aAAa,CAAC,UAAU,CAAC,CAACG,KAAI,GAAIJ,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAS;MAC7FL,QAAQ,CAACM,cAAc,CAAC,SAAS,CAAC,CAACC,KAAK,CAAC,CAAC;MAC1CP,QAAQ,CAACM,cAAc,CAAC,SAAS,CAAC,CAACE,KAAK,CAAC,CAAC;IAC9C,CAAC;IACDC,KAAKA,CAACC,GAAG,EAAC;MACN,IAAIC,MAAK,GAAIX,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAS;MAC/D,IAAIK,GAAG,IAAE,IAAI,EAAE;QACX,IAAIC,MAAM,CAACC,MAAK,GAAI,CAAC,EAAE;UACnBD,MAAK,GAAIA,MAAM,CAACE,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;UAC5Bb,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAQ,GAAIM,MAAM;QAC/D;MACJ,OAAO;QACCA,MAAK,GAAIX,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAS;QAC3DM,MAAK,GAAIA,MAAK,GAAI,EAAC,GAAID,GAAG;QAC1BV,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAQ,GAAIM,MAAM;MACnE;IACJ,CAAC;IACDG,aAAaA,CAAA,EAAE;MACX,IAAIH,MAAK,GAAIX,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC,CAACI,SAAS;MAC/D,IAAI,CAACd,QAAQ,CAACwB,MAAK,GAAI,SAAS;MAChC,IAAI,CAACxB,QAAQ,CAACyB,KAAI,GAAI,EAAE;MACxB,IAAI,CAACzB,QAAQ,CAAC0B,MAAK,GAAI,EAAE;MACzB,IAAI,CAAC1B,QAAQ,CAAC2B,QAAO,GAAI,YAAY;MACrC,IAAI,CAAC3B,QAAQ,CAAC4B,UAAS,GAAIR,MAAM;;MAEjC;MACAvB,KAAK,CAACgC,IAAI,CAAC,qDAAqD,EAAE,IAAI,CAAC7B,QAAQ,EAAE;QAC7E8B,MAAM,EAAE;UACJ,cAAc,EAAE;QACpB;MACJ,CAAC,EACAC,IAAI,CAAEC,GAAG,IAAK;QAAE;QACb