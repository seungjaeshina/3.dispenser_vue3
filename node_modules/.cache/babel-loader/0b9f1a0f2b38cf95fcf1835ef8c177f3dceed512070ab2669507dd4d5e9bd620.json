>\n                    <div class=\"handwriter_verify_text\" ref=\"verify_text\" >문자로 도착한 인증번호를 입력해 주세요.\n                    </div>\n                </div>\n\n                <input type=\"hidden\" class=\"myitem\" ref=\"myitem\" id=\"myitem\" name=\"myitem\" />\n                <input type=\"hidden\" class=\"myinput\" ref=\"myinput\" id=\"myinput\" name=\"myinput\"/>\n\n                <div class=\"phonepad_info\">\n                    <div class=\"pad_num\">  \n                        <div class=\"num1\"  v-on:click=\"goNum('1')\"></div>\n                        <div class=\"num2\"  v-on:click=\"goNum('2')\"></div>\n                        <div class=\"num3\"  v-on:click=\"goNum('3')\"></div>\n                    </div>\n                    <div class=\"pad_num\">  \n                        <div class=\"num4\"  v-on:click=\"goNum('4')\"></div>\n                        <div class=\"num5\"  v-on:click=\"goNum('5')\"></div>\n                        <div class=\"num6\"  v-on:click=\"goNum('6')\"></div>\n                    </div>\n                    <div class=\"pad_num\">  \n                        <div class=\"num7\"  v-on:click=\"goNum('7')\"></div>\n                        <div class=\"num8\"  v-on:click=\"goNum('8')\"></div>\n                        <div class=\"num9\"  v-on:click=\"goNum('9')\"></div>\n                    </div>\n                    <div class=\"pad_num\">  \n                        <div class=\"num0\"  v-on:click=\"goNum('0')\"></div>\n                        <div class=\"num_back\"  v-on:click=\"goNum('11')\"></div>\n                        <div class=\"numconfirm\"  v-on:click=\"goCheck()\"></div>\n                    </div>\n                </div>\n\n                <div class=\"handwriter_btn\" v-on:click=\"goReceiptList\">\n                </div>\n                <div class=\"handwriter_keyboard\">\n                </div>\n\n                <!-- ======== 모달창 ======== -->\n                <div class=\"modal\" id=\"modal2\" ref=\"modal2\" v-bind:class=\"{ 'modal-on': isModal }\">\n                    <div class=\"modal-content\">\n                        <h1 ref=\"msg\">먼저 사전접수를 해주세요.</h1>\n                        <button v-on:click=\"modalClose\">확인</button>\n                    </div>\n                </div>\n\n            </div><!--container끝-->\n\n        </div><!--wrap 끝-->\n\n    </div>\n</template>\n<script>\nimport { onMounted } from \"vue\";\nimport router from \"@/router\";\nimport '@/assets/css/easymenu/ehandwriter.css';\nimport axios from 'axios';\nimport functions from '@/assets/js/functions.js'\n\nexport default {\n    name: \"HandWriter\",\n    setup() {\n        onMounted(async () => {\n            await router.isReady();\n            const phoneno = document.querySelector(\"#phoneno\");\n            phoneno.focus;\n        });\n\n        return { \n\n            functions \n        }\n    },\n    data() {\n        return {\n            filter: \"All\",\n            data : '',\n            value : ''\n        };\n    },\n    methods: {\n        goBack(){\n            this.$router.go(-1);\n        },\n        goHome(){\n            this.$router.push('/');\n        },\n        goPhoneNo(){\n            this.$refs.myitem.textContent = 'phoneno'; \n            this.$refs.verify_text.style.display = 'none'; \n            this.$refs.phoneno.focus();\n        },\n        goVerify(){\n            this.$refs.verify_text.style.display = 'block'; \n            this.$refs.myitem.textContent = 'verify'; \n            this.$refs.verify.focus();\n        },\n        goVerifyNo(){\n            this.$refs.myitem.textContent = 'verify'; \n            this.$refs.verify.focus();\n        },\n        goNum(num) {\n            const item = this.$refs.myitem.textContent;\n            if (item == 'phoneno') {\n                let result = this.$refs.phoneno.textContent;\n                if (num=='11') {\n                    if (result.length > 0) {\n                        result = result.slice(0, -1); \n                        this.$refs.phoneno.textContent = result;\n                    }       \n                } else {\n                if (result.length > 12) {\n                //\n                    } else {\n                        result = this.$refs.phoneno.textContent;\n                        result = result + '' + num; \n                        result = functions.phoneNumber(result);\n                        this.$refs.phoneno.textContent = result;\n                    }\n                }    \n            } else if  (item == 'verify') {\n                let result = this.$refs.verify.textContent;\n                if (num=='11') {\n                    if (result.length > 0) {\n                        result = result.slice(0, -1); \n                        this.$refs.verify.textContent = result;\n                    }       \n                } else {\n                    result = this.$refs.verify.textContent;\n                    result = result + '' + num; \n                    this.$refs.verify.textContent = result;\n                }    \n            }\n\n        },\n        goCheck() {\n            const phoneno = this.$refs.phoneno.textContent;\n            const verify = this.$refs.verify.textContent;\n            if (phoneno.length < 11) {\n                if (verify.length > 3) {\n                    this.$router.push('/receiptlist');\n                } else {\n                    this.$refs.msg.textContent = '인증을 받아야 합니다.'; \n                    this.$refs.modal2.style.display = 'block'; \n                }\n            \n            } else {\n                this.$refs.msg.textContent = '핸드폰번호를 확인해 주세요.'; \n                this.$refs.modal2.style.display = 'block'; \n            }\n        },\n        modalClose() {\n            this.$refs.modal2.style.display = 'none'; \n        },\n        goReceiptList(){\n            var sendData2 = new FormData();\n            sendData2.append(\"epostId\", \"\");\n            sendData2.append(\"telNo\", \"\")